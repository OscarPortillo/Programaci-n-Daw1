import java.util.*;
public class Ejer34 {

	public static void main(String[] args) {
		Random azar=new Random();
		int notasAzar;
		int notas[]=new int [10];
		notasAzar=azar.nextInt(1)+10;
		int indice,pos;
		indice=0;
		pos=0;
		for( ; indice < 10; indice++){
			System.out.println("Introduciendo notas de los alumnos");
			notas[indice]=notasAzar;
		}
		for(int nota:notas){
			System.out.println("Nota: "+pos++ +" "+nota);
		}
		
		
	}//main

}//cklass
