import java.util.Random;
import java.util.Scanner;

public class Ejer40 {

	public static void main(String[] args) {
		@SuppressWarnings("unused")
		Scanner teclado = new Scanner (System.in);
		@SuppressWarnings("unused")
		Random azar=new Random();
		int num=1;
		int matriz[][][]= new int[3][4][5]	;
		int k,i,j;
		for(k=0;k<3;k++){
			for(i=0;i<4;i++){
				for(j=0;j<5;j++){
					matriz[k][i][j]=num++;
				}//tercer for
			}//segundo for
		}//primer for
		
		for(k=0;k<3;k++){
			for(i=0;i<4;i++){
				for(j=0;j<5;j++){
					System.out.print(matriz[k][i][j]+" ");
				}//tercer for
				System.out.println();
			}//segundo for
			System.out.println();
		}//primer for
	}//main

}
