import java.util.*;
public class Ejer38 {

	public static void main(String[] args) {
		@SuppressWarnings("unused")
		Scanner teclado = new Scanner (System.in);
		@SuppressWarnings("unused")
		Random azar=new Random();
		/*
		 * 38.-Cargar en una matriz las notas de los alumnos de 
		 * un colegio en función del número de
		 * cursos (filas) y del número de alumnos por curso (columnas).
		 */
		
		int vecNotas[]={10,5,9,2,4,3,5,7,8,6,4,5,1,0,2,4,5,3,2,10};
		
		//System.out.println(vecNotas.length);
		for(int nota:vecNotas){
			System.out.print(nota+" ");
		}
	}//main

}
