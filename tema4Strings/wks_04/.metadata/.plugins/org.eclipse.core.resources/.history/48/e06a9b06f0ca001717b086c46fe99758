import java.util.Random;
import java.util.Scanner;

public class Ejer40 {

	public static void main(String[] args) {
		Scanner teclado = new Scanner (System.in);
		Random azar=new Random();
		int matriz[][][]= new int[3][4][5]	;
		int k,i,j;
		for(k=0;k<3;k++){
			for(i=0;i<4;i++){
				for(j=0;j<5;j++){
					matriz[k][i][j]=azar.nextInt(10)+1;
				}//tercer for
			}//segundo for
		}//primer for
	}//main

}
