import java.util.*;
public class Ejer37 {

	public static void main(String[] args) {
		@SuppressWarnings({ "unused", "resource" })
		Scanner teclado=new Scanner(System.in);
		/*37.-Generar una matriz de 4 filas y 5 columnas 
		 * con números aleatorios entre 1 y 100, y hacer
		 * su matriz transpuesta.*/
		int vecNum[][]=new int[4][5];
		Random azar=new Random();
		System.out.println("Generando matriz");
		for(int k=0; k<4; k++){
			for(int i=0;i<5;i++){
				vecNum[k][i]=azar.nextInt(1)+100;
			}
		}
		for(int[] numero: vecNum){
			System.out.println(numero);
		}
	}//main

}//class
