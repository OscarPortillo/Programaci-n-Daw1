import java.util.*;
public class Skraaaa {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Random azar=new Random ();
		int num=azar.nextInt(5)+1;
		int i,k;
		for(i=0;i<num;i++){
			System.out.println(' ');
			for(k=num-i;k<num;k++){
				System.out.print("*");
			}
		}
	}

}
