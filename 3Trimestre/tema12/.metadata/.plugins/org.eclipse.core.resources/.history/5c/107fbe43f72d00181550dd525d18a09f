import java.util.*;
public class Principal {

	public static void main(String[] args) {
		Random azar=new Random();
		int num;
		num=azar.nextInt(15)+1;
		
		System.out.println("Se van a generar "+num+" datos");
		for(int i=0;i<num;i++){
			num=azar.nextInt(15)+1;
			System.out.print(num+" * ");
		}
	}
}
