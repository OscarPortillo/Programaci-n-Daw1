
public class Estaticos {
	private int num;

	public int getNum() {
		return num;
	}

	public void setNum(int num) {
		this.num = num;
	}
	public boolean primo(int num){
		int i,a=0;
		 for(i=1;i<(num+1);i++){
	         if(num%i==0){
	             a++;
	            }
	         }
	         if(a!=2){
	        	 return false;
	            }else{
	            	return true;
	         }
		
		
	}
	
}
