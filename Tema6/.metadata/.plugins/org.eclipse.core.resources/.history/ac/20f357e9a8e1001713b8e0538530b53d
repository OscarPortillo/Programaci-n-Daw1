
public class Estaticos {
	private int num;

	public static void primo(int num){
		int i,a=0;
		for(i=1;i<(num+1);i++){
			if(num%i==0){
				a++;
			}
		}
		if(a!=2){// si contador es distinto de 2 no es primo
			System.out.println("No es primo");
		}else{
			System.out.println("Es primo");
		}
	}// metodo ver si es primo


}//class estaticos
