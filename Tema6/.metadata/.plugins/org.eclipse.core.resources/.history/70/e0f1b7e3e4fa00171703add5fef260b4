import java.util.*;
public class Principal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Calendar fecha=new GregorianCalendar();
		Integer dia;
		Integer mes;
		Integer año;
		dia=fecha.DAY_OF_MONTH;
		mes=fecha.MONTH+2;
		System.out.println(dia+" "+mes+" "+año);
		año=fecha.YEAR+10;
		Fecha hoy=new Fecha(dia,mes,año);
		System.out.println(hoy.getMes());
		System.out.println(Fecha.esBisiesto()+" .... bisiesto");
		System.out.println(Fecha.esCorrecta()+" .... fecha ");
	}

}
