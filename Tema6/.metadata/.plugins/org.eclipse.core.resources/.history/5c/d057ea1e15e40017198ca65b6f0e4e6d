
public class Coche {
	private String color;
	private int velocidad;
	private int ruedas;
	Coche(){
		color="Rojo";
		velocidad=0;
		ruedas=4;
	}
	public void verCoche(){
		System.out.println("Color del coche: "+color);
		System.out.println("Velocidad del coche: "+velocidad);
		System.out.println("Ruedas: "+ruedas);
	}
}//class
