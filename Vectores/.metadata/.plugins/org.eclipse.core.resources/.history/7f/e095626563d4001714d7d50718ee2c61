		String letra,palabraMisteriosa;
		char palabraPrueba[];
		int contador;
		boolean acertada=false;
		
		palabraMisteriosa=vecPalabras[(int)(Math.random()*3)];
		 contador=0;
		 while(contador < 10 ){
			 
			 do{
				 System.out.println("Letra ? ");
