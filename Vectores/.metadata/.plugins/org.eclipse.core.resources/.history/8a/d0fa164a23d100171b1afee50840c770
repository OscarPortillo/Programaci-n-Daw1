import java.util.*;
public class Principal {

	public static void main(String[] args) {
		@SuppressWarnings({ "unused", "resource" })
		Scanner teclado=new Scanner(System.in);
		@SuppressWarnings("unused")
		int edad;
		@SuppressWarnings("unused")
		String nombres[]={"Oscar","María","Jose","Waldo","Rene","Alex","David","Peña","Pablo","Gonzalo","Mardo","Messi","Lionel",
				"Andrés","Alvaro","Nelly","Beatrice","Ioana","Miriam","Kelly","Katherin","Mayra","Raquel",""};
		System.out.println(nombres.length);
		int numeroNombres;

		String vecNombres[]=new String[20];
		
		
		
		for(int k=0;k<10;k++){
			numeroNombres=(int)(Math.random()*nombres.length+1);
			vecNombres[k]=nombres[numeroNombres];
			//vecNombres[k]=nombres[(int)(Math.random()*nombres.length+1)];
			System.out.println(numeroNombres);
		}

		for(int k=0;k<10;k++){
			System.out.print(vecNombres[k]+" ");//;=nombres[(int)(Math.random()*nombres.length+1)];
		}
	}//main

}//class