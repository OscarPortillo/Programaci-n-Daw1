import java.util.*;
public class Principal {

	public static void main(String[] args) {
		@SuppressWarnings("unused")
		Random azar=new Random();
		
		String vect[]=new String [3];
		for(int i = 0;i < 3;i++){
			vect [i]=new String (Libreria.pedirCadena("Introduce un nombre"));			
		}
		int cont=0;
		for(int i=0; i<vect.length;i++){	
			if(vect[i].equals("oscar")){
				cont++;
			}			
		}
		System.out.println("Oscar se repite: "+cont);
	}

}
