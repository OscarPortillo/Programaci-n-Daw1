
public class Empleado {
	private String nombre;
	private String apellido;
	private int edad;
	private double sueldo;
	public Empleado(String nombre, String apellido, int edad,double sueldo) {
		this.nombre = nombre;
		this.apellido = apellido;
		this.edad = edad;
		this.sueldo=sueldo;
	}
	
	
}//class Empleado